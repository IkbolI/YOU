<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.you">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_you"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".test.GamePage2"
            android:parentActivityName=".registration.PreGamePage"/>
        <activity android:name=".test.GamePage1"
            android:parentActivityName=".registration.PreGamePage"/>
        <activity android:name=".registration.GameList" /> <!-- <activity android:name=".gaming.GamePage12" /> -->
         <activity android:name=".test.GamePage11" />
         <activity android:name=".test.GamePage10" />
         <activity android:name=".test.GamePage9" />
         <activity android:name=".test.GamePage8" />
         <activity android:name=".test.GamePage7" />
         <activity android:name=".test.GamePage6" />
         <activity android:name=".test.GamePage5" />
         <activity android:name=".test.GamePage4" />
         <activity android:name=".test.GamePage3" />
        <activity android:name=".registration.PreGamePage"
            android:parentActivityName=".registration.GameList"
            tools:overrideLibrary="com.pierfrancescosoffritti.androidyoutubeplayer"/>
        <activity android:name=".registration.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".registration.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>